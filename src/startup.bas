5 I = 1
10 D$ = CHR$(4)
12 M$ = CHR$(27) + CHR$(15) : REM TURN SPECIAL CHARACTERS ON (80COL CARD CHARS)
15 N$ = CHR$(24) + CHR$(14) : REM TURN SPECIAL CHARACTERS OFF
18 DIM LINES$(15)

20 PRINT D$;"PR#3"

25 ONERR GOTO 100
30 ? D$;"OPEN DATA"
40 ? D$;"READ DATA"
41 INPUT L
42 INPUT H : HTAB(H)
45 INPUT V : VTAB(V)
50 INPUT A$
55 LINES$(I) = A$ : I = I + 1
60 REM HTAB(H) : PRINT M$ + A$ + N$
70 GOTO 50
100 ? D$;"CLOSE"

110 FOR I = 1 TO L
120   VTAB(I) : HTAB(H)
140   PRINT M$ + LINES$(I) + N$;
170 NEXT I

180 ? N$

200 FOR I = 1 TO L
205   A$ = LINES$(I)
210   VTAB(I)
220   FOR X = 1 TO LEN(A$)
240     IF ASC(MID$(A$, X, 1)) = 32 OR ASC(MID$(A$, X, 1)) = 126 THEN HTAB(H + X - 1) : ? " ";
260   NEXT X
270 NEXT I

280 ? D$;"RUN MAIN"
